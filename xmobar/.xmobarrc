Config { font = "Ubuntu Mono Ligaturized 14"
       , bgColor = "#0f111a"
       , fgColor = "#a6accd"
       , position = TopH 22
       , lowerOnStart = True
       , commands = [ Run Cpu ["-t", " <total>%"] 10
                    , Run Memory ["-t"," <used> MB (<usedratio>%)"] 10
                    , Run Swap ["-t", "󰯍 <used> MB"] 10
    		    , Run Date "%a %b %_d %Y %H:%M" "date" 10
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " <fc=#b480d6>󰲒</fc> }{ <fc=#e2795b>%cpu%</fc> <fc=#abcf76>%memory%</fc> <fc=#464b5d>%swap%</fc>   %date% "
       }
