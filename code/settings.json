  "terminal.integrated.cursorBlinking": true,
  "explorer.confirmDelete": false,
  "vetur.ignoreProjectWarning": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "vim.easymotionKeys": "aoeidtn',.pyfgcrlzqjkxbmuh-",
  "vim.matchpairs": "(:),{:},[:],<:>",
  "window.menuBarVisibility": "toggle",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "phase",
  "material-icon-theme.hidesExplorerArrows": true,
  "open-in-browser.default": "Mozilla Firefox",
  "editor.unicodeHighlight.allowedCharacters": {
    "Ñ€": true
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.renderWhitespace": "trailing",
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": false,
  "html.autoCreateQuotes": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "terminal.integrated.fontFamily": "'Ubuntu Mono Ligaturized', 'JetBrainsMonoNL Nerd Font', 'UbuntuMono Nerd Font Mono', 'Ubuntu Mono'",
  "vim.surround": false,
  "terminal.integrated.localEchoExcludePrograms": [
    "nvim",
    "vim",
    "vi",
    "nano",
    "tmux"
  ],
  "workbench.startupEditor": "none",
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "workbench.iconTheme": "bearded-icons",
  "workbench.colorTheme": "Bearded Theme Arc Blueberry",
  "editor.suggest.preview": true,
  "editor.rulers": [120],
  "editor.fontLigatures": true,
  "breadcrumbs.filePath": "off",
  "prettier.singleQuote": true,
  "prettier.vueIndentScriptAndStyle": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.cursorWidth": 2,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.activityBar.location": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "editor.wordWrapColumn": 120,
  "accessibility.signals.lineHasError": {
    "sound": "off"
  },
  "accessibility.signals.lineHasFoldedArea": {
    "sound": "off"
  },
  "accessibility.signals.lineHasBreakpoint": {
    "sound": "off"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "accessibility.signals.onDebugBreak": {
    "sound": "off"
  },
  "accessibility.signals.noInlayHints": {
    "sound": "off"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "editor.stickyScroll.enabled": false,
  "csharp.format.enable": false,
  "vim.easymotionMarkerForegroundColorOneChar": "#ff007c",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#00dfff",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#2b8db3",
  // "editor.fontWeight": "bold",
  "terminal.integrated.enablePersistentSessions": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.enablePreview": false
